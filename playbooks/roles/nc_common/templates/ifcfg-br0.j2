DEVICE="br0"
TYPE="Bridge"
BOOTPROTO="{{ dhcp_check.stdout }}"
ONBOOT="yes"
DELAY=0
{% if dhcp_check.stdout == 'static' %}
IPADDR="{{ ipaddr }}"
GATEWAY="{{ gateway }}"
NETMASK="{{ netmask }}"
{% endif %}
{% if nameservers is defined %}
{% for i in range(0,nameservers|length) %}
DNS{{ i + 1 }}={{ nameservers[i] }}
{% endfor %}
{% endif %}
